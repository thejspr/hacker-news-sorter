{
  "name": "Hacker News Sorter",
    "version": "1.5",
    "description": "Sort posts on Hacker News (news.ycombinator.com) by points.",
    "content_scripts": [
    {
      "matches": [
        "http://news.ycombinator.com/*"
        ],
      "js": ["content.js", "jquery-1.5.1.min.js"]
    }
  ],
    "icons": {
      "48": "icon2.png"
    },
    "permissions": [
      "http://news.ycombinator.com/*"
      ],
    "background_page": "background.html"
}
